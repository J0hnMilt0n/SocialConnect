{
  "version": 2,
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "project.settings"
  },
  "buildCommand": "python3.9 manage.py collectstatic --noinput"
}
